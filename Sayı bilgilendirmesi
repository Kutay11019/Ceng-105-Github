#include <stdio.h>
#include <stdio.lab>
int main()
int s;
printf("Please write a number:");
scanf("%d", &s);
if(s>10)
  printf("your number is bigger than ten");
else if(s<0)
  printf("your number is negative");
else if(s % 2 == 0)
  printf("you wrote an even number \n");
else 
  printf("you wrote an odd number \n");
  
if(s>=0 && s<10)
  printf("your number is a figure");

return 0;
